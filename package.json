{
  "name": "metacity",
  "displayName": "Metacity Studio",
  "private": true,
  "version": "1.0.4",
  "description": "Metacity Studio for viewing and editing 3D scenes",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "version": "npm run version:changelog && git add -A",
    "version:changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s && code CHANGELOG.md --wait",
    "release": "git switch release && git merge dev --ff-only && git push --follow-tags && git switch dev",
    "release:patch": "git switch dev && git pull && npm version patch && git push && npm run release",
    "release:minor": "git switch dev && git pull && npm version minor && git push && npm run release",
    "release:major": "git switch dev && git pull && npm version major && git push && npm run release"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.38.0",
    "@loaders.gl/core": "^4.3.3",
    "@loaders.gl/gltf": "^4.3.3",
    "@metacity/geometry": "^1.0.0",
    "@react-router/node": "^7.0.1",
    "@react-router/serve": "^7.0.1",
    "@react-spectrum/toast": "^3.0.0-beta.17",
    "@spectrum-icons/illustrations": "^3.6.17",
    "allotment": "^1.20.2",
    "csv-parse": "^5.6.0",
    "d3-scale-chromatic": "^3.1.0",
    "fast-deep-equal": "^3.1.3",
    "gl-matrix": "^3.4.3",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router": "^7.0.1",
    "shpts": "^1.0.9"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.1",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.2"
  }
}

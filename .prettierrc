{
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "htmlWhitespaceSensitivity": "ignore",
    "importOrder": [
        "^@atoms/(.*)$",
        "^@molecules/(.*)$",
        "^@organisms/(.*)$",
        "^@templates/(.*)$",
        "^@pages/(.*)$",
        "^@contexts/(.*)$",
        "^@hooks/(.*)$",
        "^@features/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
